get articles

SELECT id, title, url, date_published, text
FROM articles
WHERE date_pulled >= date('now', '-? hours') AND date_pulled < date('now');


get vocab freq

SELECT *
FROM vocab


get entity freq

SELECT *
FROM entity


get max count

SELECT count
FROM vocab
ORDER BY count DESC
LIMIT 1;


get article entity

SELECT entity_class, entity_entity, count
FROM article_entity
WHERE article_id = ?;